
Route::get('/{{routepath}}/{{modelkey}}', {{class}}::class)
    ->middleware(['web'])
    ->name('{{routename}}')
    ->crumbs(function ($crumbs, ${{item}}) {
        $crumbs->parent('{{routeparent}}', {{routeparent_parameter}})->push('{{routetitle}}', route('{{routename}}', ${{item}}->id));
    });
